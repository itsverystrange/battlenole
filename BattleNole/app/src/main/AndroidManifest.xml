<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.battlenole" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.android.battlenole.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.battlenole.InfoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.android.battlenole.Game"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.android.battlenole.SetupActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.android.battlenole.MyShips"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
